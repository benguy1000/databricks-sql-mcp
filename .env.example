# Databricks SQL MCP Server - Environment Variables
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Your Databricks workspace URL
# Azure:  https://adb-1234567890.azuredatabricks.net
# AWS:    https://my-workspace.cloud.databricks.com
# GCP:    https://my-workspace.gcp.databricks.com
DATABRICKS_HOST=

# Personal access token (generate in User Settings > Developer > Access tokens)
DATABRICKS_TOKEN=

# SQL Warehouse ID (found in the warehouse URL: /sql/warehouses/<this-id>)
DATABRICKS_WAREHOUSE_ID=
